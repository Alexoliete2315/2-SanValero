CREATE OR REPLACE PROCEDURE EJERC5(P_NOM VARCHAR2, P_LOC VARCHAR2) AS
    V_DEPT_NO DEPART.DEPT_NO%TYPE;
BEGIN
    SELECT TRUNC(MAX(DEPT_NO), -1) + 10 INTO V_DEPT_NO FROM DEPART;

    INSERT INTO DEPART (DEPT_NO, DNOMBRE, LOC) VALUES (V_DEPT_NO, P_NOM, P_LOC);

    COMMIT;

EXCEPTION
    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('ERROR GENERAL');
END EJERC5;
/
